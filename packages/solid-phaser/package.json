{
  "name": "solid-phaser",
  "version": "0.0.1-alpha.0",
  "description": "Create Phaser 3 games with solid-js",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./test": {
      "import": "./dist/test/index.es.js",
      "require": "./dist/test/index.cjs.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattjennings/solid-phaser.git"
  },
  "author": "Matt Jennings",
  "bugs": {
    "url": "https://github.com/mattjennings/solid-phaser/issues"
  },
  "homepage": "https://github.com/mattjennings/solid-phaser#readme",
  "license": "MIT",
  "scripts": {
    "dev": "WATCH=true node build.mjs",
    "build": "node build.mjs ",
    "prepublishOnly": "npm run build",
    "release": "npm publish",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.13",
    "@types/warning": "^3.0.0",
    "babel-jest": "^27.0.6",
    "canvas": "^2.8.0",
    "datauri": "^3.0.0",
    "jest": "^26.3.3",
    "phaser": "^3.55.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "solid-jest": "^0.1.1",
    "solid-js": "^1.1.0",
    "solid-testing-library": "^0.2.0",
    "typescript": "~4.2.2",
    "vite": "^2.4.4",
    "vite-plugin-solid": "^2.0.1"
  },
  "peerDependencies": {
    "phaser": "^3.55.0",
    "solid-js": "^1.0.0",
    "solid-testing-library": "^0.2.0"
  },
  "dependencies": {
    "tiny-warning": "^1.0.3"
  }
}
