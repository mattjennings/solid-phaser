    "jest": "^26.3.3",
    "phaser": "^3.55.2",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "solid-jest": "^0.1.1",
    "solid-js": "^1.1.0",
    "solid-testing-library": "^0.2.0",
    "typescript": "~4.2.2"
